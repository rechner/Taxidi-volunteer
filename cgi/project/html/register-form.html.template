	<table border="0" cellpadding="10" width="520">
		<tr><td align="left">
			<label class="description" for="parent1">Parent 1 </label>
			<input id="parent1" name="parent1" class="element text medium" type="text" maxlength="255" size=22 
			 style="font-family:verdana;font-size:10pt" />
		</td>
		
		<td align="right">
			<label class="description" for="parent2">Parent 2 </label>
			<input id="parent2" name="parent2" class="element text medium" type="text" maxlength="255" size=22
			 style="font-family:verdana;font-size:10pt" />
		</td></tr>
	</table>
	
	<table border=0 cellpadding=10 width=500>
		<tr><td align="left">
			<label class="description" for="medical">Medical </label>
            <input id="medical" name="medical" class="element text large" type="text" maxlength="520" size=45 />
		</td>
		
		<td align="right">
			<input type="checkbox" name="volunteer" value="volunteer" /> Volunteer
		</td></tr>
	</table>

	<table border=0 cellpadding=10 width=500>
		<tr><td align="left">
			<input id="submit" class="button_text" type="submit" name="submit" value="Save" style="width:100px;height:40px"/>
			<input id="cancel" class="button_text" type="button" name="cancel" value="Cancel" style="width:100px;height:40px" />
		</td></tr>
	</table>

	<input type="hidden" name="form_id" value="register" />
	</form>


</body>
</html>

